{
  "expo": {
    "name": "KJ Khandala Drivers",
    "slug": "kj-khandala-drivers",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E3A8A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kjkhandala.drivers"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#1E3A8A"
      },
      "package": "com.kjkhandala.drivers",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera for scanning QR codes"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0e590844-644a-4bcf-b76a-df7ea00f03fb"
      }
    },
    "owner": "voyagetechsolutions"
  }
}
